	font-size: 36rpx;
	/*color: #FFE600;*/
}
.grab-red-list .grab-red-num {
	width: 20%;
	color: #FFE600;
	text-align: right;
}
.grab-red-list .user-nick text:nth-of-type(2) {
	font-size: 28rpx;
	color: #FFF;
	margin-left: 10rpx;
}
.note-title {
	width: 100%;
	display: flex;
	flex-wrap: wrap;
	align-items:center;
}
.note-title view:nth-of-type(1) {
	width: 30%;
	height: 1px;
	background-color: #FFF;
	
}
.note-title view:nth-of-type(2) {
	width: 40%;
	text-align: center;
	font-size: 32rpx;
	color: #FFF;
}
.note-title view:nth-of-type(3) {
	width: 30%;
	height: 1px;
	background-color: #FFF;
}
.note-text {
	margin-top: 50rpx;
	width: 650rpx;
	padding-left: 50rpx;
}
