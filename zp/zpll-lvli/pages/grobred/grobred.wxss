	height: 60rpx;
	line-height: 60rpx;
	font-size: 28rpx;
}
.share-btn button::after {
	outline: none;
}
.grab-red-msg {
	width: 650rpx;
	padding-left: 50rpx;
	margin-top: 30rpx;
}
.grab-red-msg .grab-lead{
	text-align: center;
	font-size: 32rpx;
	color: #FFF;
	padding: 20rpx 0;
}
.grab-red-msg .grab-lead text {
	 color: #FC1C15;
	 text-decoration: underline
}
.grab-red-list {
	width: 100%;
	display: flex;
	flex-wrap: wrap;
	margin-top: 28rpx;
	line-height: 88rpx;
}
.grab-red-list .user-icon {
	width: 17%;
}
.grab-red-list .user-icon image {
	width: 88rpx;
	height: 88rpx;
}
.grab-red-list .user-nick {
	width: 63%;
	color: #FFF;
}
.grab-red-list .user-nick image {
	width: 50rpx;
	height: 38rpx;
}

