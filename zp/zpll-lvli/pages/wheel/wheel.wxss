.zp {
	width: 100%;
	position: relative;
	height: 750rpx;
	overflow: hidden;
}
.zp-box canvas {
	width: 100%;
	height: 750rpx;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 1;
}
.rotate-1 {
	animation-name: rotate;
	animation-duration: 1s;
	animation-timing-function: linear;
	animation-iteration-count: infinite;
}
.rotate-2 {
	animation-name: rotate;
	animation-duration: 1s;
	animation-timing-function: linear;
	animation-iteration-count: 2;
}
@keyframes rotate{
	from {transform: rotate(0deg);}
	to {transform: rotate(360deg);}
}
