.sign li.on.not :after{
	width: 40px;
	height: 40px;
	margin:0 0 0 -20px;
	content: "";
	position: absolute;
	top: 0;
	left: 50%;
	border-radius: 50%;
	opacity: 0;
	border:1px solid #FFBF17;
	box-sizing:border-box;
	-webkit-animation: boom .8s infinite ease-in-out;
    animation: boom .8s infinite ease-in-out;
}

@-webkit-keyframes boom {
	0% {opacity: 0}
	25% {opacity: .5}
	50% {opacity: 1}
	75% {opacity: .5}
	100% {
		-webkit-transform: scale(1.2);
		transform: scale(1.2);
		opacity: 0
	}
}
@keyframes boom {
	0% {opacity: 0}
	25% {opacity: .5}
	50% {opacity: 1}
	75% {opacity: .5}
	100% {
		transform: scale(1.2);
		transform: scale(1.2);
		opacity: 0
	}
}
