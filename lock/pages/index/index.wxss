.map{
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  box-sizing: border-box;
}
.map image{
   /* min-width:100%; */
   box-sizing: border-box;
}
.car_box{
  position: absolute;
  left: 0;
  top: 0;
}
.car{
  position: absolute;
  text-align: center;
  padding: 0;
  margin: 0;
  overflow: hidden;
  transform-origin: 0px 0px 0px;
  /* font-size:8rpx; */
}
.car image{
  position: absolute;
  top: 0;
  right: 0;
  left:0;
  bottom: 0;
  z-index: 99;
}
.car text{
  color: #000;
  text-align: center;
  -webkit-transform-origin-x: 0;
  -ms-transform-origin-x: 0;
  -o-transform-origin-x: 0;
 
  -webkit-transform: scale(0.3);
  -ms-transform: scale(0.3);
  -o-transform: scale(0.3); 
  transform: scale(0.3);   
}


.car1,.car19{
  /* 绿色 （可用）*/
  background: #4ED27E;
}
.car2{
  /* 黄色 （维修）*/
  background: #FFB800;
}
.car6{
  /* 紫色 （异常） */
  background: #FD00FC;
}
.car3,.car5,.car7,.car8,.car16,.car17{
  /* 红色 （占用）*/
  background: #FF674F;
}
.car4,.car99{
  /* 灰色 （休眠中）*/
  background: #CDCDCD;
}


.car_1,.car_19{
  /* 绿色 （可用）*/
  color:#4ED27E;
}
.car_2{
  /* 黄色 （维修）*/
  color:#FFB800;
}
.car_6{
  /*异常（紫色）  */
  color:#FD00FC;
}
.car_3,.car_5,.car_7,.car_8,.car_16,.car_17{  /**8 红色+预，16 红色+停，17 红色+停**/
  /* 红色 （占用）*/
  color:#FF674F;
}
.car_4,.car_99{
  /* 灰色 （休眠中）*/
  color:#BDBDBD;
}






