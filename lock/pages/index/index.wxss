/*开锁  */
.open_lock{
  background: -webkit-linear-gradient(right,#FF674F, #FF894B); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right,#FF674F, #FF894B); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right,#FF674F, #FF894B); /* Firefox 3.6 - 15 */
  background: linear-gradient(right,#FF674F, #FF894B); /* 标准的语法 */
}

